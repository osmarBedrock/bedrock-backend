{
    "version": 2,
    "builds": [
      {
        "src": "src/server.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "prisma/**",
            "src/**/*.ts",
            "src/**/*.js",
            "env/**"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/server.ts"
      }
    ]
  }